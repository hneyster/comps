journal = {Ecology},
keywords = {Biodiversity,Equilibrium,Extinction,Immigration,Island biogeography,Local processes,Regional processes,Spatial scale,Species richness,comps,comps1,metacommunity},
mendeley-tags = {comps,comps1,metacommunity},
