mendeley-tags = {RES 508},
number = {2},
pages = {522--529},
publisher = {Elsevier Ltd},
title = {{The importance of social drivers in the resilient provision of ecosystem services}},
url = {http://dx.doi.org/10.1016/j.gloenvcha.2010.12.004},
volume = {21},
