keywords = {Adaptaci??n cultural,An??lisis multi-nivel,Cambio de valores,Cultural adaptation,Estabilidad social,Multilevel analysis,Sistemas soco-ecol??gicos,Social stability,Social-ecological systems,Value shift},
number = {4},
