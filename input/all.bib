keywords = {a key assumption is,are set by the,colour opponency,colour thresholds,colour vision,mechanism,mechanism in which it,originates,receptor noise,spectral sensitivity,that noise in this,thus,to investigate a given,when thresholds are used},
number = {1394},
pages = {351--358},
