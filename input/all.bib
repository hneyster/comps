@article{Azhar2015,
abstract = {The expansion of commercial oil palm crop has modified much of the$\backslash$nnatural landscape, subsequently leading to biodiversity loss in$\backslash$nSoutheast Asia. Aside from large-scale oil palm monoculture plantations,$\backslash$nself-managed oil palm smallholdings are also becoming common in palm oil$\backslash$nproducing countries, but less is known about how management of the$\backslash$nsmallholdings affects faunal biodiversity. We argue that it is$\backslash$ncritically important to understand the role of habitat complexity at the$\backslash$nlocal and landscape scales for maintaining faunal biodiversity in oil$\backslash$npalm smallholdings. We used passive sampling methods to survey$\backslash$nunderstory birds, fruit bats, and butterflies in oil palm smallholdings$\backslash$non the west coast of Peninsular Malaysia. We quantified the diversity in$\backslash$neach taxon and measured in situ habitat quality and landscape metrics.$\backslash$nWe found that oil palm smallholdings located near rice fields supported$\backslash$nfewer bird species. Proximity to roads can give rise to bird and fruit$\backslash$nbat richness. Bird and fruit bat richness declined at sites with high$\backslash$ncrop density. Fruit bat richness declined, but butterfly richness$\backslash$nincreased, with the height of oil palm stands. Butterfly richness$\backslash$ndeclined with distance from riparian habitats. Decreased coverage and$\backslash$nheight of ground vegetation also negatively affected butterfly species$\backslash$nrichness. We also found that the number of farm houses is positively$\backslash$nrelated to bird, fruit bat, and butterfly species richness. Of the three$\backslash$ntaxa, only butterfly richness was positively influenced by crop$\backslash$ndiversity. We found that habitat complexity enables smallholdings to$\backslash$nsupport a diverse community of birds and butterflies, but not fruit$\backslash$nbats. These findings imply that oil palm smallholdings can be managed in$\backslash$na conservation agricultural matrix, as the smallholdings were able to$\backslash$nmaintain farmland biodiversity.},
