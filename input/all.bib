journal = {Journal of Comparative Physiology A: Neuroethology, Sensory, Neural, and Behavioral Physiology},
keywords = {Achromatic mechanisms,Chromatic mechanisms,Double cone,Single cone,Spectral sensitivity},
number = {3},
pages = {197--207},
pmid = {24366429},
title = {{The contribution of single and double cones to spectral sensitivity in budgerigars during changing light conditions}},
volume = {200},
year = {2014}
}
