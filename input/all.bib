doi = {10.1007/s11284-015-1297-8},
file = {:C$\backslash$:/Users/Owner/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sulai et al. - 2015 - Ecological Research.pdf:pdf},
