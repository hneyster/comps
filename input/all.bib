@article{Fiedler2008,
abstract = {Two decades of research on the linguistic category model (LCM) have resulted in considerable empirical evidence that can be organized by distinct paradigms: the linguistic intergroup bias, the linguistic expectancy bias, the question-answering paradigm, and language-based attribution. The present article explicates the metatheory underlying the research that was actually conducted within these paradigms. This metatheory provides the organizing structure for a review of potential LCM research that has not been conducted yet because it does not fit the common metatheory. Untapped LCM research encompasses communication failures, deception, the inclusion of measures of effective communication, and the joint determination of language and cognition by social roles and ecological constraints. Because many of these often overlooked topics are theoretically important and practically useful, they suggest themselves as prominent goals of future research. T he linguistic category model (LCM; Semin {\&} Fiedler, 1988, 1991) offers a simple answer to an old psychological and philosophical question: What is the relationship between language and cognition (Clark, 1996; Fiedler, in press; Semin, 2000)? Rather than claiming that language determines thought (i.e., the notion of linguistic relativity) or that cognition determines language and symbolic representations (i.e., universalism), the LCM suggests a two-sided answer. Refined thoughts that entail distinct patterns can be expressed by the speaker in the condensed format of single-word stimuli. However, the refined messages encoded in such condensed lexical stimuli will also affect the thoughts and cognitive processes in the listener. Thus, lexical stimuli both reflect and causally influence the communication partners' thoughts and cognitive contents.},
author = {Fiedler, Klaus},
doi = {10.1177/0261927X07313656},
journal = {Journal of Language and Social Psychology},
number = {2},
pages = {182--196},
title = {{The Implicit Meta-Theory That Has Inspired and Restricted LCM Research Why Some Studies Were Conducted but Others Not}},
url = {http://jls.sagepub.comhttp//online.sagepub.com},
volume = {27},
year = {2008}
}
