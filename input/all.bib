journal = {Animal Cognition},
keywords = {Ethics,Fish cognition,Intelligence,Pain,Sentience,Welfare},
number = {1},
pages = {1--17},
pmid = {24942105},
title = {{Fish intelligence, sentience and ethics}},
volume = {18},
year = {2015}
}
