journal = {Ecology},
keywords = {biodiversity,conservation,detection heterogeneity,occurrence heterogeneity,site-},
number = {4},
pages = {842--854},
pmid = {16676528},
title = {{Estimating Species Richness and Accumulation By Modeling Species Occurrence and Detectability}},
url = {http://dx.doi.org/10.1890/0012-9658(2006)87[842:ESRAAB]2.0.CO;2},
