keywords = {Aichi targets,South Africa,biodiversity offsets,developing countries,ecosystem services,habitat transformation,pro-tected areas},
pages = {1--9},
title = {{The conservation costs and economic benefits of using biodiversity offsets to meet international targets for protected area expansion}},
url = {https://www-cambridge-org.ezproxy.library.ubc.ca/core/services/aop-cambridge-core/content/view/C852F8EA271C84E01BFF6BD8F52A0BEC/S0030605317001521a.pdf/conservation{\_}costs{\_}and{\_}economic{\_}benefits{\_}of{\_}using{\_}biodiversity{\_}offsets{\_}to{\_}meet{\_}international{\_}targets{\_}for},
