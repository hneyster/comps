pages = {1433--46},
pmid = {9547323},
title = {{Visual pigments, oil droplets and cone photoreceptor distribution in the european starling (Sturnus vulgaris)}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9547323},
volume = {201 (Pt 9)},
