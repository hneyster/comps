@article{Nguyen2016,
abstract = {Scholars across all disciplines have long been interested in how knowledge moves within and beyond their community of peers. In conservation and natural resource management, however, we are lagging behind. Rapid environmental changes and calls for sustainable management practices mean that we urgently need to be using the best knowledge possible in forming decisions, policies, and practices to protect biodiversity and sustainably manage vulnerable natural resources. While the conservation literature on knowledge exchange (KE) and knowledge mobilization (KMb) has grown in recent years, much of it is based on context-specific case studies. This presents a challenge for learning cumulative lessons from KE/KMb research, and thus effectively using knowledge in conservation and natural resources management. While continued research on the gap between knowledge and action is valuable, overarching conceptual frameworks are now needed to enable summaries and comparisons across diverse KE/KMb research. In this essay, we propose a knowledge-action framework to provide a conceptual roadmap for future research and practice in KE/KMb, with the aim of synthesizing lessons learned from contextual case studies, and to guide the development and testing of hypotheses in this domain. The knowledge-action framework is composed of three elements that occur at multiple levels and scales: knowledge production (e.g., academia, government), the knowledge mediation sphere (e.g., knowledge networks, actors, relational dimension, contextual dimension), and knowledge-based action (e.g., instrumental, symbolic, and conceptual). The framework integrates concepts from the sociology of science in particular, and serves as a guide to further our comprehensive understanding of knowledge exchange and mobilization in conservation and sustainable natural resource management. This article is protected by copyright. All rights reserved},
annote = {NULL},
author = {Nguyen, Vivian M and Young, Nathan and Cooke, Steven J},
doi = {10.1111/cobi.12857},
file = {:C$\backslash$:/Users/Owner/Documents/Literature/Nguyen, Young, Cooke - 2016 - Conservation Biology.pdf:pdf},
issn = {1523-1739},
journal = {Conservation Biology},
keywords = {Conservation social science,Interdisciplinarity,Knowledge action,Knowledge exchange,Knowledge mobilization,Science policy,Science transfer,Sociology,community,comps,comps2},
mendeley-tags = {Sociology,community,comps,comps2},
number = {October},
pages = {n/a--n/a},
title = {{A roadmap for knowledge exchange and mobilization research in conservation and natural resource management}},
url = {http://dx.doi.org/10.1111/cobi.12857},
year = {2016}
}
